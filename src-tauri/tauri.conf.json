{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Synergism",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../..",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run tauri:dev",
    "beforeBuildCommand": "npm run tauri:build"
  },
  "app": {
    "windows": [
      {
        "title": "Synergism",
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
